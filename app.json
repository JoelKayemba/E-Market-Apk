{
  "expo": {
    "name": "E-Market",
    "slug": "E-Market",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icones/e-shop.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icones/e-shop.png",
      "resizeMode": "contain",
      "backgroundColor": "white"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Cette application utilise votre localisation pour vous fournir des services personnalis√©s."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    
    ]
  }
}
